{
    "name": "bangpound/assetic-cli",
    "license": "MIT",
    "type": "project",
    "description": "Assetic as a command line tool",
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "symfony/yaml": "~2.3",
        "cilex/console-service-provider": "2.0.*@dev",
        "bangpound/assetic-service-provider": "~1.0@dev",
        "symfony/property-access": "~2.3"
    },
    "require-dev": {
        "symfony/debug": "~2.3"
    },
    "bin": [
        "bin/assetic"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Bangpound\\Assetic\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
