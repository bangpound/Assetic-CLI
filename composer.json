{
    "name": "bangpound/assetic-cli",
    "license": "MIT",
    "type": "project",
    "description": "Assetic as a command line tool",
    "require": {
        "php": ">=5.3.3",
        "bangpound/protosilex-pimple": "~1.0@dev",
        "bangpound/assetic-service-provider": "~1.0@dev",
        "cilex/console-service-provider": "2.0.*@dev",
        "flint/tacker": "~1.1",
        "incenteev/composer-parameter-handler": "~2.0",
        "kriswallsmith/spork": "dev-master",
        "monolog/monolog": "~1.11",
        "silex/silex": "~2.0@dev",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/property-access": "~2.3",
        "symfony/yaml": "~2.3"
    },
    "require-dev": {
        "symfony/debug": "~2.3"
    },
    "bin": [
        "bin/assetic"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "autoload": {
        "psr-4": {
            "Bangpound\\Assetic\\": "src/"
        }
    },
    "extra": {
        "incenteev-parameters": [
            {
                "file": "conf/parameters.yml"
            }
        ],
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
