{
    "name": "bangpound/assetic-cli",
    "license": "MIT",
    "type": "project",
    "description": "Assetic as a command line tool",
    "require": {
        "php": ">=5.3.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "symfony/property-access": "~2.3",
        "symfony/yaml": "~2.3",
        "pimple/pimple": "3.0 as 2.1",
        "flint/tacker": "~1.1.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "bangpound/assetic-service-provider": "~1.0@dev",
        "silex/providers": "~2.0@dev",
        "silex/api": "~2.0@dev",
        "gonzalo123/yaml2pimple": "dev-master"
    },
    "require-dev": {
        "symfony/debug": "~2.3"
    },
    "bin": [
        "bin/assetic"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "autoload": {
        "psr-4": {
            "Bangpound\\Assetic\\": "src/"
        }
    },
    "extra": {
        "incenteev-parameters": [{
            "file": "conf/parameters.yml"
        },
        {
            "file": "conf/assetic.yml"

        }],
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
